{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "proprietors": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "createdAt": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "updatedAt": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "deletedAt": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "format": "date-time"
                    },
                    "userId": {
                        "type": "string"
                    },
                    "caseId": {
                        "type": "string"
                    },
                    "guardianId": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "proprietorType": {
                        "type": "string",
                        "enum": [
                            "insured",
                            "payer"
                        ]
                    },
                    "inviteType": {
                        "type": "string",
                        "enum": [
                            "email",
                            "none"
                        ]
                    },
                    "countryCode": {
                        "type": "string"
                    },
                    "citizenId": {
                        "type": "string"
                    },
                    "passportNumber": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "title": {
                        "type": "object",
                        "properties": {
                            "key": {
                                "type": "string"
                            },
                            "sex": {
                                "type": "string"
                            },
                            "code": {
                                "type": "string"
                            },
                            "label": {
                                "type": "string"
                            },
                            "value": {
                                "type": "string"
                            },
                            "translations": {
                                "type": "object",
                                "properties": {
                                    "en": {
                                        "type": "object",
                                        "properties": {
                                            "label": {
                                                "type": "string"
                                            }
                                        },
                                        "required": [
                                            "label"
                                        ]
                                    },
                                    "th": {
                                        "type": "object",
                                        "properties": {
                                            "label": {
                                                "type": "string"
                                            }
                                        },
                                        "required": [
                                            "label"
                                        ]
                                    }
                                },
                                "required": [
                                    "en",
                                    "th"
                                ]
                            }
                        },
                        "required": [
                            "key",
                            "sex",
                            "code",
                            "label",
                            "value",
                            "translations"
                        ]
                    },
                    "firstName": {
                        "type": "string"
                    },
                    "middleName": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "lastName": {
                        "type": "string"
                    },
                    "dateOfBirth": {
                        "type": "string",
                        "format": "date"
                    },
                    "phoneNumber": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "email": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "verificationRef": {
                        "type": "string"
                    },
                    "verificationCache": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string"
                            },
                            "ref": {
                                "type": [
                                    "string",
                                    "null"
                                ]
                            },
                            "status": {
                                "type": "string"
                            },
                            "userId": {
                                "type": "string"
                            },
                            "feedback": {
                                "type": [
                                    "string",
                                    "null"
                                ]
                            },
                            "meta": {
                                "type": "object",
                                "properties": {
                                    "proprietor": {
                                        "type": "object",
                                        "properties": {
                                            "lastName": {
                                                "type": "string"
                                            },
                                            "firstName": {
                                                "type": "string"
                                            }
                                        },
                                        "required": [
                                            "lastName",
                                            "firstName"
                                        ]
                                    },
                                    "insuredLastName": {
                                        "type": "string"
                                    },
                                    "insuredFirstName": {
                                        "type": "string"
                                    }
                                },
                                "required": [
                                    "proprietor",
                                    "insuredLastName",
                                    "insuredFirstName"
                                ]
                            }
                        },
                        "required": [
                            "id",
                            "meta",
                            "status",
                            "userId"
                        ]
                    },
                    "certificationNumber": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "memberNumber": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "expiresAt": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "format": "date-time"
                    },
                    "verifiedAt": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "format": "date-time"
                    }
                },
                "required": [
                    "id",
                    "createdAt",
                    "updatedAt",
                    "userId",
                    "caseId",
                    "proprietorType",
                    "inviteType",
                    "countryCode",
                    "citizenId",
                    "title",
                    "firstName",
                    "lastName",
                    "dateOfBirth",
                    "verificationRef",
                    "verificationCache"
                ],
                "if": {
                    "properties": {
                        "proprietorType": {
                            "const": "insured"
                        }
                    }
                },
                "then": {
                    "properties": {
                        "firstName": {
                            "const": "กรรณชัย"
                        },
                        "lastName": {
                            "const": "อ่างแก้ว"
                        },
                        "inviteType": {
                            "const": "email"
                        },
                        "citizenId": {
                            "const": "1729900599685"
                        },
                        "dateOfBirth": {
                            "const": "2002-12-09"
                        },
                        "email": {
                            "const": "jidapa.o@appman.co.th"
                        },
                        "properties": {
                            "verificationCache": {
                                "meta": {
                                    "firstName": {
                                        "const": "กรรณชัย"
                                    },
                                    "lastName": {
                                        "const": "อ่างแก้ว"
                                    }
                                }
                            }
                        }
                    }
                },
                "else": {
                    "properties": {
                        "firstName": {
                            "const": "อมรรัตน์"
                        },
                        "lastName": {
                            "const": "ดิษเจริญ"
                        },
                        "dateOfBirth": {
                            "const": "2001-05-21"
                        },
                        "citizenId": {
                            "const": "1239900336573"
                        }
                    }
                }
            },
            "minItems": 2,
            "maxItems": 2
        }
    },
    "required": [
        "proprietors"
    ]
}